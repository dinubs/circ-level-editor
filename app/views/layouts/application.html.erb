<!DOCTYPE html>
<html>
<head>
  <title>CircLevelEditor</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <meta name=viewport content="width=device-width, initial-scale=1">
  <% if content_for? :og_tags %>
    <%= yield :og_tags %>
  <% else %>
    <meta property="og:title" content="Circ Level Editor" />
    <meta property="og:description" 
      content="Circ Level Editor game, play levels made by the community and challenge your friends." />
    <meta property="og:site_name" content="Circ Level Editor" />
  <% end %>
</head>
<body>

<% if content_for? :navigation %>
  <%= yield :navigation %>
<% else %>
  <%= render 'shared/navigation' %>
<% end %>
<div class="wrapper">
  <%= render 'shared/flashes' %>
</div>
<%= yield %>

<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-72583199-1', 'auto');
  ga('send', 'pageview');

</script>

</body>
</html>
